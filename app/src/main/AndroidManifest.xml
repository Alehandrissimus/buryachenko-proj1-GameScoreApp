<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.buryachenko_proj1_gamescoreapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Buryachenkoproj1GameScoreApp">
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TeamsActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".LeaderboardActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".ScoreActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".WinnerActivity"
            android:screenOrientation="portrait"/>
    </application>

</manifest>